{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window - SQL only, no network access",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "dialog:default",
    "fs:allow-write-file",
    "fs:allow-read-file"
  ]
}
